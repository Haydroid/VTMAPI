<android.support.percent.PercentFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <Spinner
        android:id="@+id/RestType"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_gravity="top"
        android:popupBackground="@android:color/holo_blue_light"
        app:layout_heightPercent="12%"
        app:layout_widthPercent="50%" />


    <Button
        android:id="@+id/btnResetEject"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_gravity="top|right"
        android:text="复位"
        app:layout_heightPercent="12%"
        app:layout_widthPercent="50%" />


    <Button
        android:id="@+id/btnCardPos"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="@string/card_pos"
        app:layout_heightPercent="12%"
        app:layout_marginTopPercent="11%"
        app:layout_widthPercent="50%" />

    <Button
        android:id="@+id/btnPermitIns"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_gravity="top|right"
        android:text="@string/permit_insertion"
        android:textColor="@android:color/holo_blue_light"
        app:layout_heightPercent="12%"
        app:layout_marginTopPercent="11%"
        app:layout_widthPercent="50%" />


    <Button
        android:id="@+id/btnDenieIns"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_gravity="top|right"
        android:text="@string/denie_insertion"
        android:textColor="@android:color/holo_red_light"
        app:layout_heightPercent="12%"
        app:layout_marginTopPercent="22%"
        app:layout_widthPercent="50%" />


    <Button
        android:id="@+id/btnCardOut"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_gravity="left"
        android:text="从前端弹卡"
        app:layout_heightPercent="12%"
        app:layout_marginTopPercent="22%"
        app:layout_widthPercent="50%" />

    <Button
        android:id="@+id/btnMoveToFront"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_gravity="top|right"
        android:text="移动卡到前端持卡位"
        app:layout_heightPercent="12%"
        app:layout_marginTopPercent="33%"
        app:layout_widthPercent="50%" />

    <Button
        android:id="@+id/btnMoveToIC"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_gravity="top|right"
        android:text="移动卡到IC触点位"
        app:layout_heightPercent="12%"
        app:layout_marginTopPercent="33%"
        app:layout_widthPercent="50%" />


    <Button
        android:id="@+id/btnMoveToReadPos"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="移动到射频位"
        app:layout_heightPercent="12%"
        app:layout_marginTopPercent="33%"
        app:layout_widthPercent="50%" />


    <Button
        android:id="@+id/btnpermitin"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_gravity="left"
        android:text="@string/permit_insertionmag"
        android:textColor="@android:color/holo_blue_light"
        app:layout_heightPercent="12%"
        app:layout_marginTopPercent="44%"
        app:layout_widthPercent="50%" />

</android.support.percent.PercentFrameLayout>

